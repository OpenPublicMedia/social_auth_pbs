{
    "name": "drupal/social_auth_pbs",
    "type": "drupal-module",
    "description": "Social Auth integration for PBS.org.",
    "keywords": ["Drupal"],
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/social_auth_pbs",
    "support": {
        "issues": "https://www.drupal.org/project/issues/social_auth_pbs",
        "source": "https://git.drupalcode.org/project/social_auth_pbs"
    },
    "authors": [
        {
            "name": "Christopher C. Wells (wells)",
            "email": "drupal.org@chris-wells.net"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url":  "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^8",
        "drupal/social_auth": "^4.0",
        "openpublicmedia/oauth2-pbs": "^1.0",
        "drupal/core": "^9 || ^10"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
